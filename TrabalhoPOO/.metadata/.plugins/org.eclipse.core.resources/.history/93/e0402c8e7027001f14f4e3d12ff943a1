package Biblioteca;

public class Biblioteca {
	
	public class Main {
	    public static void main(String[] args) {
	        // Criar um novo professor
	        Professor professor = new Professor("12345678900", "João Silva", "joao.silva@example.com", "Doutorado");

	        // Exibir os dados do professor e a senha gerada
	        System.out.println("CPF: " + professor.CPF);
	        System.out.println("Nome: " + professor.nome);
	        System.out.println("Email: " + professor.email);
	        System.out.println("Titulação: " + professor.titulacao);
	        System.out.println("Senha gerada: " + professor.getSenha());
	    }
	}


}
