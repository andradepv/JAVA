package Biblioteca;

public abstract class UsuarioABS {
	
	private String CPF;
	private String nome;
	private String email;

	public UsuarioABS(String CPF, String nome, String email) {
	    this.CPF = CPF;
	    this.nome = nome;
	    this.email = email;
	    }

	    public abstract boolean login(String senha);
}
